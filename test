int main() {
   int x;
   int y;
   x = 8;
   y = 64;

   while ((x > 1) && (y > 0)) {
        print(y);
        print(" ");
        x = x - 1;
        y = y / 2;
   }

   if (x == 1) {
      print("Hello, World");
   }

   return 0;
}


class MyClass {

    int x;
    int y;

    public int foo(int a, bool s) {
        x = x + a;
        return x;
    }

}
